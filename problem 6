#include<iostream>
using namespace std;
class raja
{
    public:
    void task(int a)
    {
        cout<<"Tasks-wash the clothes,";
    }
    void task(int b,int c)
    {
        cout<<"read,";
    }
    void task(int d,int e,int f)
    {
        cout<<"book two tickets etc.";
    }
};
int main()
{
    raja obj;
    obj.task(1);
    obj.task(1,1);
    obj.task(1,1,1);
}
